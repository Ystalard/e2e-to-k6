{
  "name": "e2e-to-k6-poc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:chrome": "cypress run --browser chrome --reporter mochawesome --reporter-options reportDir=cypress/results/json,quiet=true,overwrite=false,html=false,json=true",
    "report:merge": "mochawesome-merge cypress/results/json/*.json > cypress/results/index.json",
    "report:generate": "marge cypress/results/index.json --reportDir public --assetsDir public/assets --reportPageTitle 'Cypress Report'",
    "generate:traces": "node tools/generate_traces.js",
    "generate:traces:seeded": "node tools/generate_traces.js --seed",
    "fakerize": "node tools/fakerizeNetworkLog.js",
    "test:k6:local": "k6 run k6-script.js",
    "start:test-server": "node test-site/server.js",
    "poc:local": "node tools/run_local_poc.js"
  },
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.8",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "cypress": "^12.0.0",
    "cypress-file-upload": "^5.0.8",
    
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.0.0"
  }
}
